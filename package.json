{
    "name": "autoEdit2",
    "version": "1.0.3",
    "description": "Text based video editing app",
    "main": "frontEnd/index.html",
    "chromium-args": "--mixed-context",
    "dom_storage_quota": 5,
    "window": {
        "show": true,
        "frame": true,
        "toolbar": true,
        "always_on_top": false,
        "fullscreen": false,
        "show_in_taskbar": true,
        "transparent": false,
        "icon": "nw.icns",
        "width": 800,
        "height": 670,
        "position": "mouse",
        "min_width": 400,
        "min_height": 500
    },
    "scripts": {
        "test": "jasmine",
        "start": "nw",
        "build": "node deploy.js",
        "page": "jekyll serve  --source ./docs --destination ./docs/_site --watch",
        "document_docco": "docco -o ./docs/docco_docs --title 'autoEdit 2 Documentation' ./*.js ./frontEnd/*.js ./frontEnd/date_now/*.js ./frontEnd/edl_composer/*.js ./frontEnd/srt/*.js ./frontEnd/js/*.js ./frontEnd/js/models/*.js ./frontEnd/js/views/*.js ./frontEnd/js/collections/*.js ./interactive_transcription_generator/*.js ./interactive_transcription_generator/**/*.js",
        "document_jsdoc": "./node_modules/.bin/jsdoc *.js  --destination ./docs/jsdoc_docs  ./*.js ./frontEnd/*.js ./frontEnd/date_now/*.js ./frontEnd/edl_composer/*.js ./frontEnd/srt/*.js ./frontEnd/js/*.js ./frontEnd/js/models/*.js ./frontEnd/js/views/*.js ./frontEnd/js/collections/*.js ./interactive_transcription_generator/*.js ./interactive_transcription_generator/**/*.js"
    },
    "author": "Pietro Passarelli <pietro.passarelli@gmail.com> (http://pietropassarelli.com)",
    "license": "ISC",
    "dependencies": {
        "app-module-path": "^1.1.0",
        "backbone": "^1.3.3",
        "fluent-ffmpeg": "^2.1.0",
        "install": "^0.8.1",
        "jquery": "2.1.0",
        "jsdoc": "^3.4.1",
        "level-js": "^2.2.4",
        "linvodb3": "^3.24.0",
        "lodash": "^4.15.0",
        "medea": "^1.0.3",
        "npm": "^3.10.6",
        "nw": "^0.12.3",
        "nw-builder": "^2.2.6",
        "underscore": "^1.8.3",
        "watson-developer-cloud": "^2.4.1"
    },
    "devDependencies": {}
}