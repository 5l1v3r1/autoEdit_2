{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown","plugins/summarize"],
    "markdown": {
        "excludeTags": ["author"],
        "tags": ["foo", "bar"],
        "hardwrap": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}